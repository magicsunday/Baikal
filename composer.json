{
    "name": "sabre/baikal",
    "type": "project",
    "description": "BaÃ¯kal is a lightweight CalDAV + CardDAV server based on PHP, SQLite or MySQL, and SabreDAV",
    "keywords": [
        "Project",
        "Server",
        "Calendar",
        "Contact",
        "WebDAV",
        "CalDAV",
        "CardDAV",
        "iCalendar",
        "lightweight"
    ],
    "homepage": "https://sabre.io/baikal/",
    "license": "GPL-3.0-only",
    "require": {
        "php": ">=8.2 <8.4",
        "ext-dom": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-zlib": "*",
        "sabre/dav": "~4.6.0",
        "twig/twig": "~3.8.0",
        "symfony/yaml": "^7.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.51.0",
        "rector/rector": "^0.18",
        "phpro/grumphp": "^2.4",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "replace": {
        "jeromeschneider/baikal": "self.version"
    },
    "autoload": {
        "psr-0": {
            "Baikal": "Core/Frameworks/",
            "Flake": "Core/Frameworks/",
            "Formal": "Core/Frameworks/"
        }
    },
    "support": {
        "source": "https://github.com/sabre-io/Baikal"
    },
    "scripts": {
        "cs-fixer": [
            "php-cs-fixer fix"
        ],
        "phpstan": [
            "phpstan analyse Core html"
        ],
        "test": [
            "composer cs-fixer",
            "composer phpstan"
        ]
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
